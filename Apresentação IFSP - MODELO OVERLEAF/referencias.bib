@article{Egilmez,
abstract = {OpenFlow is a Software Defined Networking (SDN) paradigm that decouples control and data forwarding layers of routing. In this paper, we propose OpenQoS, which is a novel OpenFlow controller design for multimedia delivery with end-to-end Quality of Service (QoS) support. Our approach is based on QoS routing where the routes of multimedia traffic are optimized dynamically to fulfill the required QoS. We measure performance of OpenQoS over a real test network and compare it with the performance of the current state-of-the-art, HTTP-based multi-bitrate adaptive streaming. Our experimental results show that OpenQoS can guarantee seamless video delivery with little or no video artifacts experienced by the end-users. Moreover, unlike current QoS architectures, in OpenQoS the guaranteed service is handled without having adverse effects on other types of traffic in the network.},
author = {Egilmez, H.E. and Dane, S.T.},
file = {:home/felipe/Dropbox/Mestrado/Artigos/SDN QoE QoS/2012 - OpenQoS$\backslash$: An OpenFlow Controller Design for Multimedia Delivery with End-to-End Quality of Service over Software-Defined Networks.pdf:pdf},
isbn = {1467348635},
journal = {Signal {\&} Information Processing Association Annual Summit and Conference (APSIPA ASC)},
keywords = {Computer architecture,Delay,HTTP-based multibitrate adaptive streaming,Internet,Multimedia communication,OpenFlow controller design,OpenQoS,QoS routing,Quality of service,Routing,Streaming media,data forwarding layer,end-to-end quality of service support,hypermedia,media streaming,multimedia delivery,multimedia traffic,seamless video delivery,Software-Defined Network,telecommunication network routing,telecommunication traffic,transport protocols},
mendeley-groups = {SURVEY QOS SDN},
pages = {1--8},
title = {{OpenQoS: An OpenFlow controller design for multimedia delivery with end-to-end Quality of Service over Software-Defined Networks}},
url = {http://ieeexplore.ieee.org/xpls/abs{\_}all.jsp?arnumber=6411795},
year = {2012}
}

